<% provide(:title, "新規登録") %>
<h1>新規登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@student) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :s_class %>
      <%= f.text_field :s_class %>

      <%= f.label :s_code %>
      <%= f.text_field :s_code %>

      <%= f.label :s_no %>
      <%= f.text_field :s_no %>

      <%= f.submit "アカウント登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
